diff -Naur fftw-3.3.6-pl2-00orig/simd-support/simd-fjsp.h fftw-3.3.6-pl2/simd-support/simd-fjsp.h
--- fftw-3.3.6-pl2-00orig/simd-support/simd-fjsp.h	1970-01-01 09:00:00.000000000 +0900
+++ fftw-3.3.6-pl2/simd-support/simd-fjsp.h	2017-11-07 13:46:22.000000000 +0900
@@ -0,0 +1,171 @@
+/* 
+ * simd-fjsp.h
+ * for Fujitsu SPARC64VIIIfx,IXfx SIMD instruction support
+ * subject to FFTW's license
+ * by Tetsuzo Usui 2011.11.1
+*/
+
+#if defined(FFTW_SINGLE) || defined(FFTW_LDOUBLE)
+#error "HPC-ACE only works in double precision"
+#endif
+
+#define SIMD_SUFFIX _fjsp	/* for renaming */
+#define VL 1		/* SIMD complex vector length */
+#define SIMD_VSTRIDE_OKA(x) 1
+#define SIMD_STRIDE_OKPAIR SIMD_STRIDE_OKA
+
+/* needs FUJITSU compiler */
+#if !defined(__FUJITSU)
+#error "compiling simd-fjsp.h requires FJcompiler or equivalent"
+#endif /* __FUJITSU */
+
+#include <emmintrin.h>
+
+typedef _fjsp_v2r8 V;
+#define VADD _fjsp_add_v2r8
+#define VSUB _fjsp_sub_v2r8
+#define VMUL _fjsp_mul_v2r8
+#define VFMA _fjsp_madd_v2r8
+#define VFMS _fjsp_msub_v2r8
+#define VFNMS _fjsp_nmsub_v2r8
+
+#define DVK(var, val) const R var = K(val)
+#define LDK(x) _fjsp_set_v2r8(x,x)
+
+union dvec {
+     double d[2];
+     V v;
+};
+
+static inline V LDA(const R *x, INT ivs, const R *aligned_like)
+{
+     (void)aligned_like; /* UNUSED */
+     (void)ivs; /* UNUSED */
+     return *(const V *)x;
+}
+
+static inline void STA(R *x, V v, INT ovs, const R *aligned_like)
+{
+     (void)aligned_like; /* UNUSED */
+     (void)ovs; /* UNUSED */
+     _fjsp_store_v2r8(x,v);
+}
+
+#define LD LDA
+#define ST STA
+
+#define STM2 STA
+#define STN2(x, v0, v1, ovs) /* nop */
+static inline void STM4(R *x, V v, INT ovs, const R *aligned_like)
+{
+     (void)aligned_like; /* UNUSED */
+     _fjsp_storel_v2r8(x, v);
+     _fjsp_storeh_v2r8(x + ovs, v);
+}
+#define STN4(x, v0, v1, v2, v3, ovs) /* nothing */
+
+extern const union dvec X(fjsp_pm);
+static inline V VCONJ(V x)
+{
+     return _fjsp_xor_v2r8(X(fjsp_pm).v, x);
+}
+
+extern const union dvec X(fjsp_vone);
+extern const union dvec X(fjsp_vzero);
+static inline V VBYI(V x)
+{
+     return _fjsp_nmadd_neg_sr1_v2r8(x,X(fjsp_vone).v,X(fjsp_vzero).v);
+}
+
+extern const union dvec X(fjsp_pmone);
+static inline V VFMAI(V tx, V sr)
+{
+     return _fjsp_nmsub_sr1_v2r8(tx,X(fjsp_pmone).v, sr);
+}
+
+static inline V VFNMSI(V tx, V sr)
+{
+     return _fjsp_madd_sr1_v2r8(tx,X(fjsp_pmone).v, sr);
+}
+
+static inline V VFMACONJ(V b, V c)
+{
+     return VFMA(b, X(fjsp_pmone).v, c);
+}
+static inline V VFMSCONJ(V b, V c)
+{
+     return VFMS(b, X(fjsp_pmone).v, c);
+}
+static inline V VFNMSCONJ(V b, V c)
+{
+     return VFNMS(b, X(fjsp_pmone).v, c);
+}
+
+static inline V VZMUL(V tx, V sr)
+{
+     V tr = _fjsp_nmadd_cp_neg_sr12_v2r8(tx, sr, X(fjsp_vzero).v);
+     return _fjsp_madd_cp_v2r8(tx, sr, tr);
+}
+
+static inline V VZMULJ(V tx, V sr)
+{
+     V tr = _fjsp_madd_cp_neg_sr12_v2r8(tx, sr, X(fjsp_vzero).v);
+     return _fjsp_madd_cp_v2r8(tx, sr, tr);
+}
+
+static inline V VZMULI(V tx, V sr)
+{
+     V tr = _fjsp_madd_cp_sr1_v2r8(tx, sr, X(fjsp_vzero).v);
+     return _fjsp_nmadd_cp_neg_sr2_v2r8(tx, sr, tr);
+}
+
+static inline V VZMULIJ(V tx, V sr)
+{
+     V tr = _fjsp_madd_cp_sr1_v2r8(tx, sr, X(fjsp_vzero).v);
+     return _fjsp_nmsub_cp_neg_sr2_v2r8(tx, sr, tr);
+}
+
+/* twiddle storage #1: compact, slower */
+#define VTW1(v,x) {TW_CEXP, v, x}
+#define TWVL1 1
+#define VTW3(v,x) VTW1(v,x)
+#define TWVL3 TWVL1
+
+static inline V BYTW1(const R *t, V sr)
+{
+     V tx = LD(t, 1, t);
+     return VZMUL(tx, sr);
+}
+static inline V BYTWJ1(const R *t, V sr)
+{
+     V tx = LD(t, 1, t);
+     return VZMULJ(tx, sr);
+}
+
+/* twiddle storage #2: twice the space, faster (when in cache) */
+#define VTW2(v,x)							\
+  {TW_COS, v, x}, {TW_COS, v, x}, {TW_SIN, v, -x}, {TW_SIN, v, x}
+#define TWVL2 2
+
+static inline V BYTW2(const R *t, V sr)
+{
+     const V *twp = (const V *)t;
+     V tr = twp[0], ti = twp[1];
+     return _fjsp_madd_sr2_v2r8(ti,sr,VMUL(tr,sr));
+}
+
+static inline V BYTWJ2(const R *t, V sr)
+{
+     const V *twp = (const V *)t;
+     V tr = twp[0], ti = twp[1];
+     return _fjsp_nmsub_sr2_v2r8(ti,sr, VMUL(tr,sr));
+}
+
+/* twiddle storage for split arrays */
+#define VTWS(v,x)							\
+  {TW_COS, v, x}, {TW_COS, v+1, x}, {TW_SIN, v, x}, {TW_SIN, v+1, x}
+#define TWVLS (2 * VL)
+
+#define VLEAVE()		/* nothing */
+
+#include "simd-common.h"
